		</equal>
	      </and>
	    </test>
	    <let>
	      <clip pos="2" side="tl" part="lem"/>
	      <lit v="prpers"/> <!-- vosotros -->
	    </let>
	    <let>
	      <clip pos="2" side="tl" part="gen"/>
