	  </otherwise>
	</choose>

	<choose>
	  <when>
	    <test>
	      <equal>
		<var n="tipus_verb"/>
		<lit-tag v="pron"/>
	      </equal>
	    </test>
	    <out>

	      <chunk name="det_nom_to_inf" case="caseFirstWord">
