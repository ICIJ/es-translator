	  <with-param pos="4"/>
	</call-macro>
	<choose>
	  <when>
	    <test>
	      <in caseless="yes">
		<clip pos="2" side="sl" part="lem"/>
		<list n="controlnouns"/>
	      </in>
	    </test>
	    <let>
	      <clip pos="3" side="tl" part="lem"/>
	      <lit v ="de"/>
	    </let>
	  </when> 
	  <otherwise>
