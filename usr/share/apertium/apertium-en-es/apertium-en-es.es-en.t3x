<rule comment="CHUNK: SUBJ">
      <pattern>
        <pattern-item n="SUBJ"/>
      </pattern>
      <action>
	<choose>
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="1" part="pers"/>
		  <lit-tag v="p3"/>
		</equal>
		<equal>
		  <clip pos="1" part="nbr"/>
		  <lit-tag v="sg"/>
		</equal>
	      </and>
	    </test>
	    <let>
	      <clip pos="1" part="gen"/>
	      <lit-tag v="nt"/>
	    </let>
	  </when>
	  <when>
	    <test>
	      <equal>
		<clip pos="1" part="pers"/>
		<lit-tag v="p2"/>
	      </equal>
	    </test>
	    <let>
	      <clip pos="1" part="nbr"/>
	      <lit-tag v="sp"/>
	    </let>
	    <let>
	      <clip pos="1" part="gen"/>
	      <lit-tag v="mf"/>
	    </let>
	  </when>
	  <otherwise>
	    <let>
	      <clip pos="1" part="gen"/>
	      <lit-tag v="mf"/>
	    </let>
	  </otherwise>
	</choose>
	<out>
	  <lu>
	    <clip pos="1" part="whole"/>
	  </lu>
	</out>
      </action>
</rule>



<rule comment="CHUNK: HAVER-HI">
      <pattern>
        <pattern-item n="haverhi"/>
      </pattern>
      <action>
	<choose><!--Si en el t2x s'ha posat nombre pl, cal canviar manera d'enviar verb perquè no doni error (sense persona ni nombre, coses del diccionari) -->
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="pri"/>
		</equal>
		<equal>
		  <clip pos="0" part="nbr"/>
		  <lit-tag v="pl"/>
		</equal>
	      </and>
	    </test>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="a_verb"/>
		<lit-tag v="pres"/>
	      </lu>
	    </out>
	  </when>
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="past"/>
		</equal>
		<equal>
		  <clip pos="0" part="nbr"/>
		  <lit-tag v="pl"/>
		</equal>
	      </and>
	    </test>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="a_verb"/>
		<clip pos="2" part="temps"/>
	      </lu>
	    </out>
	  </when>
	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>
	    <out>
	      <b/>
	      <lu>
		<lit v="not"/>
		<lit-tag v="adv"/>
	      </lu>
	    </out>
	  </when>
	</choose>
	<choose>
	  <when><!--segons si porta aux o no, pot haver-hi una tercera lu, envia-la si hi és -->
	    <test>
	      <or>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="cni"/>
		</equal>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="fti"/>
		</equal>
	      </or>
	    </test>
	    <out>
	      <b pos="2"/>
	       <lu>
		<clip pos="3" part="whole"/>
	      </lu>
	    </out>
	  </when>
	</choose>
      </action>
</rule>

<rule comment="CHUNK: HAVER-HI">
      <pattern>
        <pattern-item n="haverhi_pp"/>
      </pattern>
      <action>
	<choose><!--Si en el t2x s'ha posat nombre pl, cal canviar manera d'enviar verb perquè no doni error (sense persona ni nombre, coses del diccionari) -->
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="0" part="nbr"/>
		  <lit-tag v="pl"/>
		</equal>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="pri"/>
		</equal>
	      </and>		
	    </test>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="a_verb"/>
		<lit-tag v="pres"/>
	      </lu>
	      <b pos="2"/>
	    </out>
	  </when>
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="0" part="nbr"/>
		  <lit-tag v="pl"/>
		</equal>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="past"/>
		</equal>
	      </and>		
	    </test>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="lem"/>
		<clip pos="2" part="a_verb"/>
		<clip pos="2" part="temps"/>
	      </lu>
	      <b pos="2"/>
	    </out>
	  </when>

	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	      <b pos="2"/>
	    </out>
	  </otherwise>
	</choose>
	<choose><!--si és negatiu, imprimeix el 'not', sempre en 2a posició, tant si porta aux com no (there would have been, there has not been). -->
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>
	    <out>
	      <lu>
		<lit v="not"/>
		<lit-tag v="adv"/>
	      </lu>
	      <b/>
	    </out>
	  </when>
	</choose>
	<out><!--el tercer chunk sempre hi és -->
	  <lu>
	    <clip pos="3" part="whole"/>
	  </lu>
	</out>
	<choose>
	  <when><!--segons si porta aux o no, pot haver-hi una quarta lu, envia-la si hi és -->
	    <test>
	      <or>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="cni"/>
		</equal>
		<equal>
		  <clip pos="0" part="temps"/>
		  <lit-tag v="fti"/>
		</equal>
	      </or>
	    </test>
	    <out>
	      <b pos="3"/>
	       <lu>
		<clip pos="4" part="whole"/>
	      </lu>
	    </out>
	  </when>
	</choose>
      </action>
</rule>



