  <section-rules>

  <!--REGLES PER A NEGACIONS (I ADVERBIS INTERCALATS) -->

    <rule comment="CHUNK: HAVER_PP">
      <pattern>
        <pattern-item n="haver_pp"/>
      </pattern>
      <action>
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>      
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<lit v="not"/>
		<lit-tag v="adv"/>
	      </lu>
	      <b/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>  
	  </when>
	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule>




<rule comment="CHUNK: AUX VERB">
      <pattern>
        <pattern-item n="aux_verb"/>
      </pattern>
      <action>
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>      
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<lit v="not"/>
		<lit-tag v="adv"/>
	      </lu>
	      <b/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>  
	  </when>
	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule>
  
   <rule comment="CHUNK: AUX HAVER PP">
      <pattern>
        <pattern-item n="aux_haver_pp"/>
      </pattern>
      <action>
	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>      
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<lit v="not"/>
		<lit-tag v="adv"/>
	      </lu>
	      <b pos="2"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	      <b/>
	      <lu>
		<clip pos="3" part="whole"/>
	      </lu>
	    </out>  
	  </when>
	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	      <b pos="1"/>
	      <lu>
		<clip pos="2" part="whole"/>
	      </lu>
	      <b pos="2"/>
	       <lu>
		<clip pos="3" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule>

<rule comment="CHUNK: VERB CONJUGAT">
      <pattern>
        <pattern-item n="verbcj"/>
      </pattern>
      <action>
      <choose>
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="1" part="temps"/>
		  <lit-tag v="pri"/>
		</equal>
		<not>
		  <or>
		    <equal>
		      <clip pos="1" part="flexio"/>
		      <lit-tag v="p3.sg"/>
		    </equal>
		    <equal>
		      <clip pos="1" part="flexio"/>
		      <lit-tag v="p1.sg"/>
		    </equal>
		  </or>
		</not>
	      </and>
	    </test>
	    <let>
		<clip pos="1" part="temps"/>
		<lit-tag v="pres"/>
	    </let>
	    <let>
		<clip pos="1" part="flexio"/>
		<lit v=""/>
	    </let>
	  </when>
	</choose>
	
	<choose>
	  <when>
	    <test>
	      <and>
		<equal>
		  <clip pos="1" part="temps"/>
		  <lit-tag v="past"/>
		</equal>
		<not>
		  <or>
		    <equal>
		      <clip pos="1" part="flexio"/>
		      <lit-tag v="p3.sg"/>
		    </equal>
		    <equal>
		      <clip pos="1" part="flexio"/>
		      <lit-tag v="p1.sg"/>
		    </equal>
		  </or>
		</not>
	      </and>
	    </test>
	    <let>
		<clip pos="1" part="temps"/>
		<lit-tag v="past"/>
	    </let>
	    <let>
		<clip pos="1" part="flexio"/>
		<lit v=""/>
	    </let>
	  </when>
	</choose>

	<choose>
	  <when>
	    <test>
	      <equal>
		  <clip pos="1" part="temps"/>
		  <lit-tag v="pii"/>
		</equal>
	    </test>
	    <let>
		<clip pos="1" part="temps"/>
		<lit-tag v="past"/>
	    </let>
	    <let>
		<clip pos="1" part="flexio"/>
		<lit v=""/>
	    </let>
	  </when>
	</choose>


	<choose>
	  <when>
	    <test>
	      <equal>
		<clip pos="0" part="a_SV"/>
		<lit-tag v="SVneg"/>
	      </equal>
	    </test>  
	    <choose>
	      <when>
		<test>
		  <or>
		    <equal>
		      <clip pos="0" part="a_verb"/>
		      <lit-tag v="vbser"/>
		    </equal>
		    <equal>
		      <clip pos="0" part="a_verb"/>
		      <lit-tag v="vaux"/>
		    </equal>
		  </or>
		</test>
		<out>
		  <lu>
		    <clip pos="1" part="whole"/>
		  </lu>
		  <b/>
		  <lu>
		    <lit v="not"/>
		    <lit-tag v="adv"/>
		  </lu>
		</out>
	      </when>
	      <otherwise>
		<choose>
		  <when>
		    <test>
		      <and>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="pri"/>
			</equal>
			<equal>
			  <clip pos="0" part="pers"/>
			  <lit-tag v="p3"/>
			</equal>
			<equal>
			  <clip pos="0" part="nbr"/>
			  <lit-tag v="sg"/>
			</equal>
		      </and>
		    </test>
		    <let>
		      <var n="temps_aux"/>
		      <lit-tag v="pri.p3.sg"/>
		    </let>
		    <let>
		      <clip pos="1" part="temps"/>
		      <lit-tag v="inf"/>
		    </let>
		    <let>
		      <clip pos="1" part="pers"/>
		      <lit v=""/>
		    </let>
		    <let>
		      <clip pos="1" part="nbr"/>
		      <lit v=""/>
		    </let>
		  </when>
		  <when>
		    <test>
		      <or>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="pri"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="prs"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="imp"/>
			</equal>
		      </or>
		    </test>
		    <let>
		      <var n="temps_aux"/>
		      <lit-tag v="pres"/>
		    </let>	
		  </when>
		  <when>
		    <test>
		      <or>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="cni"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="ifi"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="pii"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="pis"/>
			</equal>
			<equal>
			  <clip pos="0" part="temps"/>
			  <lit-tag v="past"/>
			</equal>
		      </or>
		    </test>
		    <let>
		      <var n="temps_aux"/>
		      <lit-tag v="past"/>
		    </let>
		    <let>
		      <clip pos="1" part="temps"/>
		      <lit-tag v="inf"/>
		    </let>
		  </when>
		</choose>
		<out>
		  <lu>
		    <get-case-from pos="1">
		      <lit v="do"/>
		    </get-case-from>
		    <lit-tag v="vbdo"/>
		    <var n="temps_aux"/>
		  </lu>
		  <b/>
		  <lu>
		    <lit v="not"/>
		    <lit-tag v="adv"/>
		  </lu>
		  <b/>
		</out>
		<choose>
		  <when>
		    <test>
		      <equal>
			<case-of pos="1" part="lem"/>
			<lit v="Aa"/>
		      </equal>
		    </test>
		    <modify-case>
		      <clip pos="1" part="lem"/>
		      <lit v="aa"/>
		    </modify-case>
		  </when>
		</choose>
		<out>
		  <lu>
		    <clip pos="1" part="whole"/>
		  </lu>
		</out>
	      </otherwise>
	    </choose>
	  </when>
	  <otherwise>
	    <out>
	      <lu>
		<clip pos="1" part="whole"/>
	      </lu>
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule>

