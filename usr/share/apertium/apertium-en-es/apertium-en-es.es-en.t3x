      </def-attr>


      <def-attr n="flexio">
          <attr-item tags="p1.sg"/>
          <attr-item tags="p1.pl"/>
          <attr-item tags="p2.sg"/>
          <attr-item tags="p2.pl"/>
          <attr-item tags="p2.sp"/>
          <attr-item tags="p3.sg"/>
          <attr-item tags="p3.pl"/>
      </def-attr>


 <def-attr n="a_SV">
          <attr-item tags="SVneg"/>
          <attr-item tags="SV"/>
</def-attr>     


  </section-def-attrs>
